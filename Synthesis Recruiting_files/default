typeof b?b:function(){}};ra.prototype.translate=function(a,b,c,e){var f,g;f=[];var h=this.language||e;if(!h[a])return this.notFoundCallback(Error("Missing context : {"+a+"}, {"+b+"}")),b;e=h[a][b];if(!e)return this.notFoundCallback(Error("Missing key : {"+a+"}, {"+b+"}")),b;b=e.pluralVars?e.pluralVars.length:0;g=e.vars?e.vars.length:0;if(0<b){for(a=0;a<b;a++)f.push(h.pluralFormFunction(c[e.pluralVars[a]]));f=e.message[f.join("_")];for(a=0;a<b;a++)f=f.replace(RegExp("#"+e.pluralVars[a],"g"),this.escapeStringReplacePlacement(c[e.pluralVars[a]]))}else f=
e.message;if(0<g)for(a=0;a<g;a++)f=f.replace(RegExp("#"+e.vars[a],"g"),this.escapeStringReplacePlacement(c[e.vars[a]]));return f};ra.prototype.escapeStringReplacePlacement=function(a){return"string"===typeof a?a.replace(/\$/g,"$$$"):a};ra.prototype.hasTranslation=function(a,b,c){c=c||this.language;return!!(c&&c[a]&&c[a][b])};"undefined"!==typeof module&&(module.exports=ra);b.eventEmitter=new EventEmitter;b.eventEmitter.on("error",function(a){c.handleError("EventEmitter on error",a.fileName,a.lineNumber,
a.stack)});b.loggingHandler=new w;b.eventHandler=new X;b.activityManager=new G;b.scheduler=new O;b.main=new U;b.languageParser=new ra(x.language);b.socketManager=new aa;b.sessionHandler=new P;b.chatHandler=new M;b.agents=new ja;b.visitorHandler=new E;b.audioPlayer=new K;f=new Q;b.fileUploadHandler=new Da;void 0!==x.downloaded?(b.eventHandler.clearEvents(),b.eventEmitter.removeAllListeners(),b=null):(x.downloaded=!0,b.main.load())}catch(ya){c.handleError(ya.message,"tawk.js",ya.lineNumber||ya.line,
ya.stack)}}).call(this,window.$_Tawk);

})(window);